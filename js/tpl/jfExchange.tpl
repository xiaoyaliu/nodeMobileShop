<a  href="/jfAddressList/{{=it.goods_info.goods_id}}.html">
<div class="address-Info" style="display:block">
        <div class="address_mo">
            <div class="address_div">
                <p class="peopleMessage">
                    <span class="name">{{=it.AllAddress.consignee}}</span>
                    <span class="phone">{{=it.AllAddress.mobile}}</span>
                </p>
                <p class="addressMessage">
                    <span>{{=it.AllAddress.provinceName}}</span>
                    <span>{{=it.AllAddress.cityName}}</span>
                    <span>{{=it.AllAddress.districtName}}</span>
                    <span>{{=it.AllAddress.address}}</span>
                </p>
            </div>
            <span class="ord-img-rig"><img class="jfExchange-i" src="/image/area_return3.png" alt=""></span>
        </div>
    </div>
    <input type="hidden" value="{{=it.AllAddress.address_id}}" data-attach-point="address">

</a>

    <ul class="cartList ord_cont_bot" style="display: block">
        <li>
            <div class="displayBox">
                <div class="cart1Box">
                    <a><img src="{{=it.goods_info.phone_img}}"></a>
                </div>
                <div class="cart2Box">
                    <p>{{=it.goods_info.goods_name}}</p>
                    <p class="jfExchange-Cart2">积分:<span>{{=it.goods_info.integral_num}}</span></p>
                </div>
            </div>
            <div class="jfExchangeNum">
                <span>数量:x<span>1.00</span></span><br>
            </div>
        </li>
        <input type="hidden" value="{{=it.goods_info.goods_id}}" data-attach-point="goodsId">
    </ul>

    <div class="jfExchangeText">
        <textarea placeholder="给商家留言(选填)" name="" id="" data-attach-point="bz"></textarea>
    </div>
    <p class="bot-gray-b"></p>

    <div class="jfExchangeText">
       <p>积分: <span style="color: red;">{{=it.goods_info.integral_num}}</span></p>
    </div>
    <p class="bot-gray-b"></p>

    <div class="jfBtnBar">
        <span class="jfBtnBar-span">共 <span>1</span>件,合计: <span style="color: red;"><i>{{=it.goods_info.integral_num}}</i>积分</span></span>
        <input type="hidden" value="{{=it.goods_info.integral_num}}" data-attach-point="integral">
        <span class="jfBtnBar-submit" data-attach-point="submit">提交订单</span>
    </div>